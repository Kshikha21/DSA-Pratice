	  QueueUsing2Stacks<Integer> sq=new QueueUsing2Stacks<>();
	  int a[]= {10,20,30,40};
	  for(int elem:a) {
		  sq.push(elem);
	  }
	  System.out.println("size of the queue: "+sq.getSize());
	  
     System.out.println("First elemet of the queue: "+sq.top());
     
     while(!sq.isEmpty()) {
    	 System.out.println(sq.pop());
     }
	 

}
